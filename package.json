{
  "name": "sugar-dating-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "prod": "next export",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "postrelease": "git push && git push --follow-tags && npm run build",
    "validate": "npm run type-check && npm run build && npm run lint && npm run test:coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.6",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "jest-css-modules-transform": "^4.2.0",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run type-check && npm run format:fix && npm run lint:fix"
  }
}
